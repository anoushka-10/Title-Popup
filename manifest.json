{
  "manifest_version": 3,
  "name": "Tab Title Display",
  "version": "1.0",
  "description": "Displays the current tab's title in a popup",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Show Tab Title"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png"
  }
}